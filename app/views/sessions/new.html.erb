<div class="bg-primary-200 w-full md:w-2/3 p-4">
    <%= form_for :user, url: '/login' do |f| %>
        Email: <%= f.text_field :email %>
        Password: <%= f.password_field :password %>
        Password Confirmation: <%= f.password_field :password_confirmation %>
    <%= f.submit "Submit" %>
    <% end %> 
    <%= link_to image_tag("btn_google_signin_light_normal_web.png"), '/auth/google_oauth2', method: 'post' %>
</div>